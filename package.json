{
  "name": "kaffee-revamp",
  "private": true,
  "author": "Duke Ahn, Vitaly Blotski, Clay Hilgert, Darren Pavel",
  "description": "A Kafka broker monitor",
  "version": "1.2.0",
  "type": "module",
  "homepage": ".",
  "main": "build/electron/main.js",
  "scripts": {
    "dev": "mvn -f server/pom.xml spring-boot:run & vite",
    "start": "electron-forge start",
    "build": "vite build & cd server && mvn clean package",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "postinstall": "electron-builder install-app-devs",
    "electron:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder"
  },
  "build": {
    "extends": null,
    "files": [
      "**/*",
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraResources": [
      {
        "from": "backend",
        "to": "."
      }
    ]
  },
  "dependencies": {
    "@stomp/stompjs": "^7.0.0",
    "csv-parser": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "font-awesome": "^4.7.0",
    "help": "^3.0.2",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.19.0",
    "rpmbuild": "^0.0.23",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@stomp/stompjs": "^7.0.0",
    "@tanstack/react-query": "^5.8.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/electron-devtools-installer": "^2.2.5",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "chart.js": "^4.3.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^28.1.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "global-jsdom": "^9.1.0",
    "happy-dom": "^7.6.0",
    "jsdom": "^22.1.0",
    "msw": "^2.0.8",
    "net": "^1.0.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "react-chartjs-2": "^5.2.0",
    "react-redux": "^8.1.3",
    "react-router": "^6.15.0",
    "redux-toolkit": "^1.1.2",
    "sass": "^1.65.1",
    "sass-loader": "^13.3.2",
    "sockjs": "^0.3.24",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "stream-http": "^3.2.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.5",
    "vite": "^4.4.5",
    "vitest": "^0.34.6",
    "vitest-fetch-mock": "^0.2.2",
    "wait-on": "^7.2.0"
  }
}
