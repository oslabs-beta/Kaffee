{
  "name": "kaffee",
  "version": "1.0.0",
  "description": "Kafka Metrics and Diagnostics Tool",
  "main": "webpack.config.js",
  "directories": {
    "test": "test"
  },
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm ./ts-server/server.ts & webpack-dev-server --config webpack.config.js",
    "start:both": "concurrently \"npm run start:ts\" \"npm run start:java\"",
    "start:ts": "NODE_ENV=prod node ./ts-server/server.js",
    "start:java": "java -jar ./target/server-0.0.1-SNAPSHOT.jar",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:ts && npm run build:java",
    "build:ts": "tsc && webpack --config ./webpack.config.js",
    "build:java": "cd java-server/server && mvn clean package && cp -r target ../../build",
    "dev": "NODE_ENV=\"dev\" concurrently  \"nodemon --loader ts-node/esm ./ts-server/server.ts\" \"webpack-dev-server --config webpack.config.js\" \"cd java-server/server && mvn spring-boot:run\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
