{
  "name": "kaffee-revamp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "devFront": "vite",
    "dev": "npm start & vite",
    "build": "vite build",
    "build:java": "cd server && mvn clean package && java -cp -r target/server-1.1.1-SNAPSHOT.jar",
    "start": "cd server && mvn spring-boot:run",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write 'src/**/*.{js,jsx,css,html}'",
    "preview": "vite preview",
    "start:both": "concurrently \"vite\" \"cd build/target && java -jar server-1.1.1-SNAPSHOT.jar\"",
    "start:both-prod": "concurrently \"vite dist --port 3030 --host 0.0.0.0\" \"cd target && java -jar server-1.1.1-SNAPSHOT.jar\"",
    "test": "vitest"
  },
  "dependencies": {
    "@stomp/stompjs": "^7.0.0",
    "csv-parser": "^3.0.0",
    "font-awesome": "^4.7.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router": "^6.15.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@reduxjs/toolkit": "^1.9.5",
    "@stomp/stompjs": "^7.0.0",
    "@tanstack/react-query": "^5.8.4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "chart.js": "^4.3.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "global-jsdom": "^9.1.0",
    "happy-dom": "^7.6.0",
    "jsdom": "^22.1.0",
    "net": "^1.0.2",
